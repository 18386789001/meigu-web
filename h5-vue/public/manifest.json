{
  "name": "JPMX - Trustworthy platform",
  "short_name": "JPMX",
  "description": "Welcome to JPMX",
  "start_url": "/h5/",
  "display": "standalone",
  "background_color": "#0a0a0a",
  "theme_color": "#1e40af",
  "orientation": "portrait-primary",
  "scope": "/h5/",
  "lang": "zh-CN",
  "categories": ["finance", "business"],
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "16x16 32x32 48x48",
      "type": "image/x-icon"
    },
    {
      "src": "/favicon.ico",
      "sizes": "192x192",
      "type": "image/x-icon",
      "purpose": "any maskable"
    },
    {
      "src": "/favicon.ico",
      "sizes": "512x512",
      "type": "image/x-icon",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/favicon.ico",
      "sizes": "512x512",
      "type": "image/x-icon",
      "form_factor": "narrow"
    }
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+demo",
      "url": "/h5/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/h5/",
      "accept": {
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/h5/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Forex Trading",
      "short_name": "Forex",
      "description": "Quick access to forex trading page",
      "url": "/h5/trading/forex",
      "icons": [
        {
          "src": "/favicon.ico",
          "sizes": "32x32",
          "type": "image/x-icon"
        }
      ]
    },
    {
      "name": "Cryptocurrency",
      "short_name": "Crypto",
      "description": "Quick access to cryptocurrency trading page",
      "url": "/h5/trading/crypto",
      "icons": [
        {
          "src": "/favicon.ico",
          "sizes": "32x32",
          "type": "image/x-icon"
        }
      ]
    },
    {
      "name": "Stock Trading",
      "short_name": "Stocks",
      "description": "Quick access to stock trading page",
      "url": "/h5/trading/stocks",
      "icons": [
        {
          "src": "/favicon.ico",
          "sizes": "32x32",
          "type": "image/x-icon"
        }
      ]
    }
  ]
}

